@using BlogSystem.Web.Areas.Posts.Models

@model IEnumerable<PostViewModel>

@{
    ViewBag.Title = "Posts Home Page";
}

<div class="progress" id="loading" style="display: none;">
    <div class=" progress-bar progress-bar-danger progress-bar-striped" role="progressbar" aria-valuenow="90" aria-valuemin="0" aria-valuemax="100" style="width: 80%">
        <span class="sr-only">90% Complete (danger)</span>
    </div>
</div>

@Html.DisplayForModel(@Model)
